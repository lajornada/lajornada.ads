<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="lajornada.ads"
    purge="False">

    <configlet
        title="La Jornada ads"
        action_id="adsSettings"
        appId="lajornada.ads"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@ads-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>